<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.bennyhuo.tieguanyin.sample">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.bennyhuo.tieguanyin.sample.MainActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".GenerateBothActivity"
            android:theme="@style/AppTheme"/>
        <activity android:name=".JavaActivity"/>
        <activity android:name=".KotlinActivity"/>
        <activity android:name=".FragmentContainerActivity"/>
        <activity android:name=".transitions.DetailsActivity"/>
        <activity android:name=".inner.OutterClass$InnerClass"/>
    </application>

</manifest>